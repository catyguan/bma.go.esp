{
	"acl" : {
		"http/smm.ui" : {
			"Type": "mix", 
			"Rules" : [
				{ "Type":"pass", "Users":["*@127.0.0.1"] },
				{ "Type":"deny", "Users":["*@*"] }
			]
		}
	},
	"goluaServ" : {
		"GoLua" : {
			"smm.ui" : {
				"FL" : {
					"Type" : "file",
					"Dirs" : ["${CWD}/../golua.src"]
				},
				"Startup" : "smm.ui:startup.lua"
			}
		}
	},
	"goluaMux" : {
		"App" : [
			{
				"Name" : "smm.ui",
				"Location" : "/smm.ui/",
				"Script" : "smm.ui:httpmain.lua",
				"IndexName" : "index.gl"
			}
		]
	}
}